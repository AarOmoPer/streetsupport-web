<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <location path="about.html"><system.webServer><httpRedirect enabled="true" destination="/about/" httpResponseStatus="Permanent" /></system.webServer></location>
  <location path="about-our-team.html"><system.webServer><httpRedirect enabled="true" destination="/about-our-team/" httpResponseStatus="Permanent" /></system.webServer></location>
  <location path="category.html"><system.webServer><httpRedirect enabled="true" destination="/category/$Q" httpResponseStatus="Permanent" /></system.webServer></location>
  <location path="category-by-day.html"><system.webServer><httpRedirect enabled="true" destination="/category-by-day/$Q" httpResponseStatus="Permanent" /></system.webServer></location>
  <location path="contact.html"><system.webServer><httpRedirect enabled="true" destination="/contact/" httpResponseStatus="Permanent" /></system.webServer></location>
  <location path="donate.html"><system.webServer><httpRedirect enabled="true" destination="/donate/" httpResponseStatus="Permanent" /></system.webServer></location>
  <location path="emergency-help.html"><system.webServer><httpRedirect enabled="true" destination="/emergency-help/" httpResponseStatus="Permanent" /></system.webServer></location>
  <location path="find-help.html"><system.webServer><httpRedirect enabled="true" destination="/find-help/" httpResponseStatus="Permanent" /></system.webServer></location>
  <location path="give-help.html"><system.webServer><httpRedirect enabled="true" destination="/give-help/" httpResponseStatus="Permanent" /></system.webServer></location>
  <location path="give-items.html"><system.webServer><httpRedirect enabled="true" destination="/give-items/" httpResponseStatus="Permanent" /></system.webServer></location>
  <location path="give-items-submit-details.html"><system.webServer><httpRedirect enabled="true" destination="/give-items-submit-details/$Q" httpResponseStatus="Permanent" /></system.webServer></location>
  <location path="how-it-started.html"><system.webServer><httpRedirect enabled="true" destination="/how-it-started/" httpResponseStatus="Permanent" /></system.webServer></location>
  <location path="join-streetsupport.html"><system.webServer><httpRedirect enabled="true" destination="/join-streetsupport/" httpResponseStatus="Permanent" /></system.webServer></location>
  <location path="organisation.html"><system.webServer><httpRedirect enabled="true" destination="/organisation/$Q" httpResponseStatus="Permanent" /></system.webServer></location>
  <location path="sponsor.html"><system.webServer><httpRedirect enabled="true" destination="/sponsor/" httpResponseStatus="Permanent" /></system.webServer></location>
  <location path="volunteer.html"><system.webServer><httpRedirect enabled="true" destination="/volunteer/" httpResponseStatus="Permanent" /></system.webServer></location>
  <system.webServer>
    <httpErrors errorMode="Custom" existingResponse="Auto" defaultResponseMode="ExecuteURL" >
      <remove statusCode="404"/>
      <remove statusCode="500"/>
      <error statusCode="404" responseMode="ExecuteURL" path="/404/" />
      <error statusCode="500" responseMode="ExecuteURL" path="/500/" />
    </httpErrors>
    <rewrite>
      <rules>
        <rule name="Force HTTPS" enabled="true">
          <match url="(.*)" ignoreCase="false" />
          <conditions>
            <add input="{HTTPS}" pattern="off" />
          </conditions>
          <action type="Redirect" url="https://{HTTP_HOST}/{R:1}" appendQueryString="true" redirectType="Permanent" />
        </rule>
        <rule name="Canonical Hostname" stopProcessing="false">
          <match url="(.*)" />
          <conditions logicalGrouping="MatchAll" trackAllCaptures="false">
            <add input="{HTTP_HOST}" pattern="^(www\.)(.*)$" />
          </conditions>
          <action type="Redirect" url="http://{C:2}{REQUEST_URI}" redirectType="Permanent" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
